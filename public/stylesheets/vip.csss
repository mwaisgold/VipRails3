}

/*--
GALERIA VIP
-------------------------------------*/

#carrusel{position:absolute; left:0;}
#imgPrincipal{cursor:pointer;  background:transparent url(http://img.mlstatic.com/org-img/vip3/layer/loading_small.gif) center no-repeat;}
#Layer{background-color:#000; opacity:.5; width:100%; height:100%; z-index:1000; position:fixed; .position:absolute; top:0; left:0;}
#Contenedor{width:600px; height:550px; background-color:#fff; border:1px solid #ccc; position:fixed; .position:absolute; z-index:2000; border-radius:5px; -ms-border-radius:5px; -moz-border-radius:5px; -webkit-border-radius:5px; text-align:center;}
#imgPpalModal{width:320px; background:transparent url(http://img.mlstatic.com/org-img/vip3/layer/loading_small.gif) center no-repeat; margin:115px auto 25px auto;}
#closer{position:absolute; top:10px; right:10px; color:#00f; text-decoration:underline; cursor:pointer; padding-right:23px; background:transparent url(http://img.mlstatic.com/org-img/vip3/layer/close.gif) right center no-repeat; display:block; height:20px; line-height:19px;}

.clickModal{width:250px; height:300px; position:absolute; top:100px; cursor:pointer;}
#clicizq:hover{background:transparent url(assets/leftModal.gif) left top no-repeat; left:0;}
#clicder:hover{background:transparent url(assets/rightModal.gif) right top no-repeat; right:0;}


#galeria {
width: 280px; height: 340px;
margin-left:10px;
_margin-left:13px; /* para IE6 */
float: left;
overflow: hidden;
position: relative;
padding:5px;
float:left;
border: 1px solid #ccc;
}
#galeria img {
width: 280px;
}
#galeria .thumb {
border-top: 1px solid #ccc;
padding:10px;
position: absolute;
bottom: 0px; left: 0;
}
.thumb {
background: #fff;
overflow: hidden;
width: 270px;
height: 40px;
position:relative;
}
#carrusel {
margin-left: 0;
width: 500px;
z-index: 1;
}
#galeria .thumb #carrusel img {
border: 1px solid #fff;
margin-left: 13px;
cursor:pointer;
width: auto;
}

#thumbsModal img{
border: 1px solid #fff;
margin-right:10px;
cursor:pointer;
};

#galeria .thumb #carrusel img.selected, #thumbsModal img.selected {
border: 1px solid #ccc;
}
#galeria .thumb #carrusel img:hover, #thumbsModal img:hover {
border: 1px solid #ccc;
}


.thumb .prev, .thumb .next {
cursor: pointer;
height: 52px; width: 16px;
text-indent: -9999px;
position: absolute;
top: 4px;
}

.thumb .prev {
background: #fff url(../assets/arrow_left_over.gif) no-repeat right center;
left: 0;
z-index: 100;
}
.thumb .next {
background: #fff url(../assets/arrow_right_over.gif) no-repeat left center;
right: 0;
z-index: 100;
}

/*--
MODAL
-------------------------------------*/
#modalOverlay {
background-color: #666;
display: block;
height: 100%;
opacity: 0.3;
position: fixed;
top: 0; left: 0;
width: 100%;
z-index: 200;
}

#modalWrap {
top: 20%; left: 23%;
padding: 10px 10px 25px 10px;
position: fixed;
z-index: 9999;
width: 650px; height: 400px;
}

#modalLoader {
background: #fff;
width: 200px;
height: 50px;
padding: 70px 0;
position: fixed;
top: 300px; left: 500px;
text-align: center;
z-index: 300;
}
#modalWrap #modalInner {
background: #fff;
border: 1px solid #666;
height: 100%;
width: 100%;
position: relative;
padding: 10px;

border-radius: 5px;
-moz-border-radius: 5px;
-webkit-border-radius: 5px;
}
#modalWrap #modalInner #modalContent {
overflow: hidden;
height: 100%;
width: 100%;
}
#modalWrap #modalInner #modalContent img {
padding: 15px 0;
width: 400px;
}
#modalWrap #modalInner #modalContent #wrapModalThumb {
overflow: hidden;
margin: 0 95px;
padding: 0 10px;
width: 190px;
}



